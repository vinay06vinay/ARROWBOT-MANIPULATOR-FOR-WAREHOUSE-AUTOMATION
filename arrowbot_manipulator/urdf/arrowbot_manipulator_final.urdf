      <origin rpy="0 0 0" xyz="-0.00036659 0.00064155 -0.00027052"/>
      <mass value="0.25482"/>
      <inertia ixx="8.9194E-05" ixy="1.8116E-07" ixz="3.6435E-08" iyy="0.00018279" iyz="2.423E-07" izz="0.00011103"/>
