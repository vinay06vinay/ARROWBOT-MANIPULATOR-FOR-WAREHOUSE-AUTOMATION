    <axis xyz="0 0 0" />
    <origin xyz="0.0 0.045 -0.015" rpy="-1.57 -1.57 0"/>
    <parent link="ur5_wrist_3_link"/>
    <child link="camera"/>
  </joint>

<!--Camera-->
