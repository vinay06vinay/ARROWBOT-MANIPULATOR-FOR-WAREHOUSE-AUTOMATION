      </inertial>
    </link>
    <joint name="left_end_joint" type="revolute">
      <parent link="ur5_wrist_3_link" />
      <child link="vacuum_effector" />
      <origin rpy="0 0 0" xyz="0 0.0 0" />
      <limit effort="30" velocity="1.0" lower="0" upper="0" />
