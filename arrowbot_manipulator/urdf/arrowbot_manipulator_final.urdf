  <!-- ROS-Industrial 'base' frame: base_link to UR 'Base' Coordinates transform -->
  <link name="ur5_base"/>
  <joint name="ur5_base_link-base_fixed_joint" type="fixed">
    <!-- Note the rotation over Z of pi radians: as base_link is REP-103
           aligned (ie: has X+ forward, Y+ left and Z+ up), this is needed
           to correctly align 'base' with the 'Base' coordinate system of
           the UR controller.
      -->
    <origin rpy="0 0 3.141592653589793" xyz="0 0 0"/>
    <parent link="ur5_base_link"/>
    <child link="ur5_base"/>
  </joint>

  <link name="base_laser">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.00029774 0.0066667 0.00013047"/>
      <mass value="0.15717"/>
      <inertia ixx="6.7885E-05" ixy="-1.3987E-07" ixz="-8.1554E-07" iyy="0.00013173" iyz="-9.0932E-08" izz="7.1972E-05"/>
